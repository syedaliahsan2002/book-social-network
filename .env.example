# Copy to .env and fill the values before deploying
APP_DOMAIN=app.example.com
API_DOMAIN=api.example.com
AUTH_DOMAIN=auth.example.com
ACME_EMAIL=you@example.com

# Database
MYSQL_ROOT_PASSWORD=change-me-strong
MYSQL_DATABASE=book_social_network_v2

# Keycloak admin
KEYCLOAK_ADMIN=admin
KEYCLOAK_ADMIN_PASSWORD=change-me-strong

# Backend profile and DB URL (normally leave default)
ACTIVE_PROFILE=dev
DB_URL=jdbc:mysql://mysql:3306/book_social_network_v2

# Optional: set image tags if you want to pull instead of build
# BACKEND_IMAGE=ghcr.io/<owner>/book-backend:latest
# FRONTEND_IMAGE=ghcr.io/<owner>/book-frontend:latest
